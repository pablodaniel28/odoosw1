<odoo>
  <data>
      <record model="ir.ui.view" id="colegio.materia_index">
      <field name="name">colegio materia list</field>
      <field name="model">colegio.materia</field>
      <field name="arch" type="xml">

         <tree>
          <field name="nombre" />

        </tree>
      </field>
    </record>

  
    <record id="colegio.materia_create" model="ir.ui.view">
      <field name="name">colegio.materia.form</field>
      <field name="model">colegio.materia</field>
      <field name="arch" type="xml">
        <form string="materiaform">
          <sheet>
            <group>
                <field name="nombre" />
                   
              
            </group>
            <footer>
            </footer>
          </sheet>
        </form>
      </field>
    </record>

    <record model="ir.actions.act_window" id="colegio.action_materia_window">
     
      <field name="name">MATERIAS</field>
      <field name="res_model">colegio.materia</field>
      <field name="view_mode">tree,form</field>
    </record> 

  </data>
</odoo> 